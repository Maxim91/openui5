{
	"sap.app": {
		"id": "card.bundle.list",
		"type": "card",
		"i18n": "i18n/i18n.properties"
	},
	"sap.card": {
		"type": "List",
		"header": {
			"title": "{{title}}",
			"subTitle": "{{subtitle}}"
		},
		"content": {
			"data": {
				"request": {
					"url": "./mockdata/employees.json"
				}
			},
			"item": {
				"title": "{Title}",
				"description": "{Position}",
				"icon": {
					"src": "{Icon}"
				},
				"actions": [
					{
						"type": "Navigation"
					}
				]
			}
		}
	}
}